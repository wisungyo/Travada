<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <View
        android:id="@+id/view_detail_riwayat_item_card_expired"
        android:background="@drawable/bg_detail_riwayat_item_card_menunggu"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:layout_width="match_parent"
        android:layout_height="95dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/tv_detail_riwayat_item_expired"
        android:text="Travaplan kamu sedang diproses, tunggu verifikasi dari Travaplan untuk ke proses berikutnya"
        android:fontFamily="@font/roboto"
        android:textSize="14dp"
        android:textColor="#474747"
        android:layout_marginTop="24dp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintTop_toTopOf="@id/view_detail_riwayat_item_card_expired"
        app:layout_constraintStart_toStartOf="@id/view_detail_riwayat_item_card_expired"
        app:layout_constraintEnd_toEndOf="@id/view_detail_riwayat_item_card_expired" />

</androidx.constraintlayout.widget.ConstraintLayout>